  {
    "name": "Tabs Viewer",
    "version": "1.0",
    "description": "One click to view all tabs",
    "permissions": [
      "contextMenus",
      "activeTab",
      "storage",
      "tabs",
      "clipboardRead",
      "clipboardWrite"
    ],
    "browser_action": {
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      },
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "commands": {

    },
    "manifest_version": 2
  }